<script>
  document.addEventListener('DOMContentLoaded', function () {
    document.querySelectorAll('.acc-content').forEach(function (acc) {
      let readMoreBtn = acc.querySelector('.read-more-btn');
      let readLessBtn = acc.querySelector('.read-less-btn');
      let fullDesc = acc.querySelector('.full-description');
      let shortDesc = acc.querySelector('.short-description');

      if (readMoreBtn) {
        readMoreBtn.addEventListener('click', function () {
          fullDesc.style.display = 'block';
          shortDesc.style.display = 'none';
          readMoreBtn.style.display = 'none';
          readLessBtn.style.display = 'inline-block';
        });
      }

      if (readLessBtn) {
        readLessBtn.addEventListener('click', function () {
          fullDesc.style.display = 'none';
          shortDesc.style.display = 'block';
          readMoreBtn.style.display = 'inline-block';
          readLessBtn.style.display = 'none';
        });
      }
    });
  });
</script>

    function showVariantContent(id) {
      blocks.forEach((block) => {
        block.style.display = block.dataset.variantId === id ? 'block' : 'none';
      });
    }

    // Default load
    showVariantContent(variantSelect.value);

    // On variant change
    variantSelect.addEventListener('change', function () {
      showVariantContent(this.value);
    });
  });
</script>


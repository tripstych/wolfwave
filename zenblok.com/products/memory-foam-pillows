<script>
  document.addEventListener('DOMContentLoaded', function () {
    const variantSelect = document.querySelector('[name="id"]');
    const blocks = document.querySelectorAll('.variant-accordion-block');

    function showVariantContent(id) {
      blocks.forEach((block) => {
        block.style.display = block.dataset.variantId === id ? 'block' : 'none';
      });
    }

    // On page load
    showVariantContent(variantSelect.value);

    // On variant change
    variantSelect.addEventListener('change', function () {
      showVariantContent(this.value);
    });

    // Accordion functionality
    document.addEventListener('click', function (e) {
      if (e.target.closest('.acc-btn')) {
        let btn = e.target.closest('.acc-btn');
        let content = btn.nextElementSibling;
        let icon = btn.querySelector('.acc-icon');

        btn.classList.toggle('active');

        if (btn.classList.contains('active')) {
          content.style.maxHeight = content.scrollHeight + 'px';
          icon.textContent = '-';
        } else {
          content.style.maxHeight = null;
          icon.textContent = '+';
        }
      }
    });

    document.querySelectorAll('.acc-btn').forEach((btn) => {
      let content = btn.nextElementSibling;
      let icon = btn.querySelector('.acc-icon');

      if (content.style.maxHeight) {
        icon.textContent = '-';
      }
    });
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    const variantSelect = document.querySelector('[name="id"]');
    const blocks = document.querySelectorAll('.variant-accordion-block1');

    function showVariantContent(id) {
      blocks.forEach((block) => {
        block.style.display = block.dataset.variantId === id ? 'block' : 'none';
      });
    }

    // Default load
    showVariantContent(variantSelect.value);

    // On variant change
    variantSelect.addEventListener('change', function () {
      showVariantContent(this.value);
    });
  });
</script>


<style>
/* Active thumbnail border */
.thumbnail-slider .slick-slide.slick-current {
  border: 2px solid #000;
  border-radius: 6px;
}

/* Hidden thumbnail no border */
.thumbnail-slider .slick-slide.hidden-thumb {
  opacity: 0 !important;
  border: none !important;
}


</style>
<script>
document.addEventListener('DOMContentLoaded', function () {
  const waitForSlick = setInterval(() => {
    if (typeof $ !== 'undefined' && typeof $.fn.slick !== 'undefined') {
      clearInterval(waitForSlick);

      /* ----------------- INIT MAIN SLIDER -------------------- */
      $('.main-slider')
        .on('init', function () {
          document.querySelectorAll('.main-slider .slick-slide').forEach((slide, i) => {
            slide.dataset.realIndex = i;
          });
        })
        .on('afterChange', function (event, slick, currentSlide) {
          /* Match active thumbnail border */
          $('.thumbnail-slider .slick-slide').removeClass('slick-current');
          $(`.thumbnail-slider .slick-slide[data-slick-index="${currentSlide}"]`).addClass('slick-current');
        })
        .slick({
          slidesToShow: 1,
          slidesToScroll: 1,
          dots: false,
          infinite: false,
          arrows: true,
          prevArrow: `
            <button class="custom-prev">
              <svg width="20" height="20" viewBox="0 0 24 24" fill="none">
                <path d="M15 6L9 12L15 18" stroke="currentColor" stroke-width="2"/>
              </svg>
            </button>
          `,
          nextArrow: `
            <button class="custom-next">
              <svg width="20" height="20" viewBox="0 0 24 24" fill="none">
                <path d="M9 6L15 12L9 18" stroke="currentColor" stroke-width="2"/>
              </svg>
            </button>
          `
        });

      /* ----------------- INIT THUMBNAIL SLIDER ----------------- */
      $('.thumbnail-slider')
        .on('init', function () {
          document.querySelectorAll('.thumbnail-slider .slick-slide').forEach((slide, i) => {
            slide.dataset.realIndex = i;
          });
        })
        .slick({
          slidesToShow: 5,
          slidesToScroll: 1,
          arrows: false,
           focusOnSelect: false,  // <<< CLICK DISABLED
          infinite: false,
          vertical: true,
          verticalSwiping: true,
          responsive: [{ breakpoint: 1068, settings: { vertical: false,  focusOnSelect: false,  verticalSwiping: false, slidesToShow: 5 } }],
        });

      document.querySelector('.thumbnail-slider').style.display = 'flex';

      /* ----------------- COLOR FILTER (MAIN SLIDER NO COLLAPSE) -------------------- */
      function updateImagesByColor(selectedColor) {
        const lower = selectedColor.toLowerCase();

        /* MAIN SLIDER */
        document.querySelectorAll('.main-slider .slick-slide').forEach((slide) => {
          const img = slide.querySelector('img');
          const alt = img ? img.alt.toLowerCase() : '';

          // Ensure we only toggle the 'hidden-slide' class if the alt text doesn't match the selected color
          if (alt && !alt.includes(lower)) {
            slide.classList.add('hidden-slide'); // Hide
          } else {
            slide.classList.remove('hidden-slide'); // Show
          }
        });
// Disable click on thumbnails completely
document.querySelectorAll('.thumbnail-slider .slick-slide').forEach(slide => {
  slide.addEventListener('click', function(e) {
    e.preventDefault();
    e.stopPropagation();
    return false;
  });
});

        /* THUMBNAILS */
        document.querySelectorAll('.thumbnail-slider .slick-slide').forEach((slide) => {
          const img = slide.querySelector('img');
          const alt = img ? img.alt.toLowerCase() : '';

          // Similarly handle thumbnails
          if (alt && !alt.includes(lower)) {
            slide.classList.add('hidden-thumb'); // Hide
          } else {
            slide.classList.remove('hidden-thumb'); // Show
          }
        });

        // Recalculate positions
        $('.main-slider').slick('setPosition');
        $('.thumbnail-slider').slick('setPosition');

        /* Auto go to first visible MAIN */
        setTimeout(() => {
          const visibleSlides = document.querySelectorAll('.main-slider .slick-slide:not(.hidden-slide)');
          if (visibleSlides.length > 0) {
            const correctIndex = visibleSlides[0].dataset.slickIndex || visibleSlides[0].dataset.realIndex;
            $('.main-slider').slick('slickGoTo', Number(correctIndex));
          }
        }, 150);
      }

      /* ----------------- THUMBNAIL HOVER → CHANGE MAIN IMAGE ------------------------ */
      document.querySelectorAll('.thumbnail-slider .slick-slide').forEach((slide) => {
        slide.addEventListener('mouseenter', function () {
          if (this.classList.contains('hidden-thumb')) return;

          const slickIndex = this.dataset.slickIndex || this.dataset.realIndex;
          $('.main-slider').slick('slickGoTo', Number(slickIndex));
        });
      });

      /* ----------------- COLOR CHANGE EVENT ------------------------ */
      document.querySelectorAll('[name="Color"]').forEach((radio) => {
        radio.addEventListener('change', function () {
          updateImagesByColor(this.value);
        });
      });

      const checkedColor = document.querySelector('[name="Color"]:checked');
      if (checkedColor) updateImagesByColor(checkedColor.value);
    }
  }, 10);
});

 /* -------------------------------------------------------
   UPDATE ARROWS ENABLE / DISABLE BASED ON VISIBLE SLIDES
--------------------------------------------------------*/
function updateArrows() {
    const mainSlider = $('.main-slider');
    const slickObj = mainSlider.slick('getSlick');

    // All visible (non-hidden) slides
    const visible = document.querySelectorAll('.main-slider .slick-slide:not(.hidden-slide):not(.slick-cloned)');

    if (visible.length === 0) return;

    // Find current visible index
    const current = slickObj.currentSlide;
    const firstVisibleIndex = Number(visible[0].dataset.slickIndex);
    const lastVisibleIndex  = Number(visible[visible.length - 1].dataset.slickIndex);

    // Enable both by default
    $('.custom-prev').prop('disabled', false).css('opacity', 1);
    $('.custom-next').prop('disabled', false).css('opacity', 1);

    // Disable LEFT
    if (current <= firstVisibleIndex) {
        $('.custom-prev').prop('disabled', true).css('opacity', 0.8);
    }

    // Disable RIGHT
    if (current >= lastVisibleIndex) {
        $('.custom-next').prop('disabled', true).css('opacity', 0.8);
    }
}

/* Trigger arrow update after change */
$('.main-slider').on('afterChange', function () {
    updateArrows();
});

</script> 


    <script>
      document.addEventListener('DOMContentLoaded', function () {
        const swatches = document.querySelectorAll('.color-swatch, .size-swatch');
        const currentURL = window.location.href;

        swatches.forEach(function (swatch) {
          if (swatch.href === currentURL) {
            swatch.classList.add('active');
            const label = swatch.previousElementSibling; // Get the span before the swatch
            if (label && label.classList.contains('new-label')) {
              label.classList.add('active');
            }
          }
        });
      });
    </script>
